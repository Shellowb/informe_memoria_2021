 number = {12},
 pages = {2006--2017},
 publisher = {Association for Computing Machinery},
 title = {{Human-in-the-loop data integration}},
 url = {https://dl.acm.org/doi/abs/10.14778/3137765.3137833},
 volume = {10},
 year = {2017}
}

