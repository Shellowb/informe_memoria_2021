 keywords = {Higher education,Student information system,User satisfaction,Visual aesthetics},
 month = {9},
 number = {8},
 pages = {1604--1622},
 publisher = {Emerald Group Holdings Ltd.},
 title = {{Student information system satisfaction in higher education: the role of visual aesthetics}},
 url = {https://www.emerald.com/insight/content/doi/10.1108/K-08-2017-0297/full/html},
 volume = {47},
 year = {2018}
}

